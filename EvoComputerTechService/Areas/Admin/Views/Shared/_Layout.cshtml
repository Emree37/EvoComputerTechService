<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">
    <title>Blank Page | Equation - Multipurpose Bootstrap Dashboard Template </title>
    <link rel="icon" type="image/x-icon" href="~/admin-assets/assets/img/favicon.ico" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700' rel='stylesheet' type='text/css'>
    <link href="~/admin-assets/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/admin-assets/assets/css/plugins.css" rel="stylesheet" type="text/css" />

    <link href="~/admin-assets/assets/css/ecommerce/view_cart.css" rel="stylesheet" type="text/css">


    <!-- END GLOBAL MANDATORY STYLES -->
    <!--  BEGIN CUSTOM STYLE FILE  -->
    <link href="~/admin-assets/assets/css/ecommerce/product-catalog.css" rel="stylesheet" type="text/css" />

    <link href="~/admin-assets/assets/css/ecommerce/product-details-1.css" rel="stylesheet" type="text/css" />

    @await RenderSectionAsync("Styles", required: false)
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM STYLES -->
    <!-- END PAGE LEVEL PLUGINS/CUSTOM STYLES -->

</head>
<body class="default-sidebar">

    <!-- Tab Mobile View Header -->
    <partial name="Partials/_tab-mobile-view" />
    <!-- Tab Mobile View Header -->
    <!--  BEGIN NAVBAR  -->
    <partial name="Partials/_navbar" />
    <!--  END NAVBAR  -->
    <!--  BEGIN MAIN CONTAINER  -->
    <div class="main-container" id="container">

        <div class="overlay"></div>
        <div class="cs-overlay"></div>


        <!--  BEGIN SIDEBAR  -->

        <partial name="Partials/_begin-main" />

        <!--  END SIDEBAR  -->
        <!--  BEGIN CONTENT PART  -->
        <div id="content" class="main-content">
            <div class="container">
                <br />
                <!-- CONTENT AREA -->
                @RenderBody()

                <!-- CONTENT AREA -->

            </div>
        </div>
        <!--  END CONTENT PART  -->
    </div>
    <!-- END MAIN CONTAINER -->
    <!--  BEGIN FOOTER  -->
    <partial name="Partials/_footer" />

    <!--  END FOOTER  -->
    <!--  BEGIN CONTROL SIDEBAR  -->
    <partial name="Partials/_begin-sidebar" />

    <!--  END CONTROL SIDEBAR  -->
    <!-- BEGIN GLOBAL MANDATORY SCRIPTS -->
    <script src="~/admin-assets/assets/js/libs/jquery-3.1.1.min.js"></script>
    <script src="~/admin-assets/bootstrap/js/popper.min.js"></script>
    <script src="~/admin-assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/admin-assets/plugins/scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/admin-assets/assets/js/app.js"></script>
    <script src="~/admin-assets/plugins/blockui/jquery.blockUI.min.js"></script>




    <script>
        $(document).ready(function () {
            App.init();
        });
    </script>

    <script>
        $(function () { $('[data-toggle="tooltip"]').tooltip() })
    </script>

    <script src="~/admin-assets/assets/js/custom.js"></script>

    <script src="~/admin-assets/assets/js/ecommerce/custom-ecommerce_pro_detail1.js"></script>

    @await RenderSectionAsync("Scripts", required: false)

    <!-- END GLOBAL MANDATORY SCRIPTS -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
</body>
</html>

